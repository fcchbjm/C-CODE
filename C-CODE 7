//			if (i % n == 0)
//				break;
//		}
//		if (i == n)
//		{
//			printf("%d\n", i);
//			m++;
//		}
//	}
//	printf("在100到200之间一共有%d个素数。\n",m);
//	return 0;
//}

//另一种更优解
#include <math.h>
int main()
{
	int i = 0;
	int m = 0;
	for (i = 101; i <= 200; i+=2)
	{
		int n = 0;
		for (n = 2; n <= sqrt(i); n++)//sqrt()函数表示开平方，同时需要引用库函数<math.h>
		{
			if (i % n == 0)
				break;
		}
		if (n > sqrt(i))
		{
			printf("%d\n", i);
			m++;
		}
	}
	printf("在100到200之间一共有%d个素数。\n", m);
	return 0;
}
