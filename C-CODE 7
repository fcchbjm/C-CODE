//	else if (a < b)
//		c = b;
//	else
//	{
//		printf("error");
//		exit(1);
//	}
//	for (d = c; d>=2; d--)
//	{
//		if (a % d == 0 && b % d == 0)
//		{
//			printf("%d和%d的最大公约数为%d", a, b, d);
//			exit(0);
//		}
//	}
//	printf("%d和%d之间没有最大公约数。\n", a, b);
//	exit(2);
//	return 0;
//}
//（另一种更优解（辗转相除法））
int main()
{
	int a = 0;
	int b = 0;
	int c = 0;
	printf("请输入需要求最大公约数的两个整数：");
	scanf("%d%d", &a, &b); 
	while (a % b)
	{
		c = a % b;
		a = b;
		b = c;
	}
	printf("最大公约数为%d",b);
	return 0;
}

